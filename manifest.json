{
    "manifest_version": 3,
    "name": "Crowdly Styler",
    "version": "1.0",
    "description": "Изменяет стили элементов Crowdly AI.",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  
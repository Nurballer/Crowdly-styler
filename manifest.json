{
  "manifest_version": 3,
  "name": "Crowdly Styler",
  "version": "1.1",
  "description": "改变 Crowdly AI 元素的样式",
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
